#include <unistd.h>

void	comb(void)
{
	int[3] = {48, 48, 48};

	while (set[0] <= 57)
	{
		if ((set[0] < set[1]) && (set[1] < set[2]))
		{
			write(1, "set[0]", 1);
			write(1, "set[1]", 1);
			write(1, "set[2]", 1);
			write(1, ",", 1);
			if (set[2]++ >= 57)
			{
				write(1, " ", 1);
			}
		}
		if (set[2]++ >= 57)
		{
			set[2] = 48;
			set[1]++;
		}
		if (set[1] == 58)
		{
			set[1] = 48;
			set[0]++;
		}
	}
}

int	main()
{
	comb();
	return (0);
}
